{
  "manifest_version": 3,
  "name": "AI Chat Navigator",
  "version": "1.0",
  "description": "为Gemini/ChatGPT等AI对话添加带有搜索和收藏功能的高级导航面板",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://gemini.google.com/*",
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}